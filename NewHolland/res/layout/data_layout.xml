        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_weight="0" >

                <Button
                    android:id="@+id/resetButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="0"
                    android:background="@drawable/button_back"
                    android:drawableLeft="@drawable/ic_cancel"
                    android:text="Limpiar"
                    android:textColor="@color/corporative_sec_color" />

 				<com.movetothebit.newholland.android.widgets.MultiSelectSpinner
                    android:id="@+id/typeSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"                    
                    android:prompt="@string/type_prompt" />
                <com.movetothebit.newholland.android.widgets.MultiSelectSpinner
                    android:id="@+id/areaSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"                    
                    android:prompt="@string/area_prompt" />
                <com.movetothebit.newholland.android.widgets.MultiSelectSpinner
                    android:id="@+id/dealerSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"                    
                    android:prompt="@string/dealer_prompt" />

                <com.movetothebit.newholland.android.widgets.MultiSelectSpinner
                    android:id="@+id/salesmanSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"                   
                    android:prompt="@string/salesman_prompt" />

                <com.movetothebit.newholland.android.widgets.MultiSelectSpinner
                    android:id="@+id/populationSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"                 
                    android:prompt="@string/population_prompt" />

                <com.movetothebit.newholland.android.widgets.MultiSelectSpinner
                    android:id="@+id/modelSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"                 
                    android:prompt="@string/model_prompt" />
                
                <com.movetothebit.newholland.android.widgets.MultiSelectSpinner
                    android:id="@+id/modelCompSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"                 
                    android:prompt="@string/modelComp_prompt" />

                <com.movetothebit.newholland.android.widgets.MultiSelectSpinner
                    android:id="@+id/periodSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"                    
                    android:prompt="@string/period_prompt" />

                <com.movetothebit.newholland.android.widgets.MultiSelectSpinner
                    android:id="@+id/brandSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                   
                    android:prompt="@string/brand_prompt" />

                <Button
                    android:id="@+id/chartButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="0"
                    android:background="@drawable/button_back"
                    android:text="Filtrar"
                    android:textColor="@color/corporative_sec_color" />

            </LinearLayout>

            <ScrollView
                android:id="@+id/scrollView1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <TableLayout
                            android:id="@+id/totalTable"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="3dp"
                            android:layout_weight="1"
                            android:background="@drawable/table_back"
                            android:padding="5dp" >

                            <TableRow
                                android:id="@+id/TableRow04"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="0"
                                android:padding="5dp"
                                android:shrinkColumns="*"
                                android:stretchColumns="*" >

                                <TextView
                                    android:id="@+id/TextView11"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center_horizontal"
                                    android:text="@string/total_title_table"
                                    android:textAppearance="@style/tableTitleText"
                                    android:textSize="18dp"
                                    android:textStyle="bold" />
                            </TableRow>

                            <TableRow
                                android:id="@+id/tableRow1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:layout_weight="0"
                                android:padding="5dp"
                                android:shrinkColumns="*"
                                android:stretchColumns="*" >

                                <TextView
                                    android:id="@+id/TextView01"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center_horizontal"
                                    android:textSize="18dp"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/textView11"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="@string/total_title_col1"
                                    android:textAppearance="@style/tableTitleColumnText" />
                            </TableRow>

                            <TableRow
                                android:id="@+id/tableRow2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp" >

                                <TextView
                                    android:layout_weight="1"
                                    android:text="@string/total_title_row1"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:id="@+id/mTotalText"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textAppearance="@style/tableRowText"
                                    android:textStyle="bold" />
                            </TableRow>

                            <TableRow
                                android:id="@+id/TableRow02"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:padding="5dp" >

                                <TextView
                                    android:layout_weight="1"
                                    android:text="@string/total_title_row2"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:id="@+id/knownTotalText"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textAppearance="@style/tableRowText"
                                    android:textSize="18dp"
                                    android:textStyle="normal" />
                            </TableRow>

                            <TableRow
                                android:id="@+id/TableRow03"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:padding="5dp" >

                                <TextView
                                    android:layout_weight="1"
                                    android:text="@string/total_title_row3"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:id="@+id/ofertTotalText"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textAppearance="@style/tableRowText"
                                    android:textSize="18dp" />
                            </TableRow>

                            <TableRow
                                android:id="@+id/TableRow01"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:padding="5dp" >

                                <TextView
                                    android:layout_weight="1"
                                    android:text="@string/total_title_row4"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:id="@+id/winTotalText"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textAppearance="@style/tableRowText"
                                    android:textSize="18dp" />
                            </TableRow>
                        </TableLayout>

                        <TableLayout
                            android:id="@+id/objetivesTable"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="3dp"
                            android:layout_weight="1"
                            android:background="@drawable/table_back"
                            android:padding="5dp" >

                            <TableRow
                                android:id="@+id/TableRow07"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="0"
                                android:padding="5dp"
                                android:shrinkColumns="*"
                                android:stretchColumns="*" >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center_horizontal"
                                    android:text="@string/objetives_title_table"
                                    android:textAppearance="@style/tableTitleText" />
                            </TableRow>

                            <TableRow
                                android:id="@+id/TableRow10"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:layout_weight="0"
                                android:padding="5dp"
                                android:shrinkColumns="*"
                                android:stretchColumns="*" >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center_horizontal" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="@string/calc_title_table"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="@string/objetives_title_col1"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="@string/objetives_title_col2"
                                    android:textAppearance="@style/tableTitleColumnText" />
                            </TableRow>

                            <TableRow
                                android:id="@+id/TableRow09"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp" >

                                <TextView
                                    android:layout_weight="1"
                                    android:text="@string/objetives_title_row1"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="(2)/(1)"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:id="@+id/indexKnownText"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textAppearance="@style/tableRowText" />

                                <TextView
                                    android:id="@+id/objetiveKnownText"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="--"
                                    android:textAppearance="@style/tableRowText" />
                            </TableRow>

                            <TableRow
                                android:id="@+id/TableRow05"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:padding="5dp" >

                                <TextView
                                    android:layout_weight="1"
                                    android:text="@string/objetives_title_row2"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="(3)/(2)"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:id="@+id/indexPresenceText"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textAppearance="@style/tableRowText" />

                                <TextView
                                    android:id="@+id/objetivePresenceText"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="--"
                                    android:textAppearance="@style/tableRowText" />
                            </TableRow>

                            <TableRow
                                android:id="@+id/TableRow08"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:padding="5dp" >

                                <TextView
                                    android:layout_weight="1"
                                    android:text="@string/objetives_title_row3"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="(4)/(3)"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:id="@+id/indexEffectText"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textAppearance="@style/tableRowText" />

                                <TextView
                                    android:id="@+id/objetiveEffectText"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="--"
                                    android:textAppearance="@style/tableRowText" />
                            </TableRow>

                            <TableRow
                                android:id="@+id/TableRow06"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:padding="5dp" >

                                <TextView
                                    android:layout_weight="1"
                                    android:text="@string/objetives_title_row5"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="(4)/(1)"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:id="@+id/indexMarketText"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:textAppearance="@style/tableRowText" />

                                <TextView
                                    android:id="@+id/objetiveMarketText"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="--"
                                    android:textAppearance="@style/tableRowText" />
                            </TableRow>
                        </TableLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="350dp"
                        android:layout_gravity="center"
                        android:gravity="center_horizontal" >

                        <com.movetothebit.newholland.android.charts.PenetrationChartView
                            android:id="@+id/penetrationChartView"
                            android:layout_width="800dp"
                            android:layout_height="350dp"
                            android:layout_gravity="center" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/chartLayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <com.movetothebit.newholland.android.charts.OverlayChartView
                            android:id="@+id/presenceChartView"
                            android:layout_width="match_parent"
                            android:layout_height="300dp"
                            android:layout_weight="1" />

                        <com.movetothebit.newholland.android.charts.OverlayChartView
                            android:id="@+id/effectivenessChartView"
                            android:layout_width="match_parent"
                            android:layout_height="300dp"
                            android:layout_weight="1" />

                        <com.movetothebit.newholland.android.charts.OverlayChartView
                            android:id="@+id/marketChartView"
                            android:layout_width="match_parent"
                            android:layout_height="300dp"
                            android:layout_weight="1" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/infoLayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <com.movetothebit.newholland.android.charts.BarChartView
                            android:id="@+id/lostChartView"
                            android:layout_width="match_parent"
                            android:layout_height="300dp"
                            android:layout_weight="1" />

                        <TableLayout
                            android:id="@+id/missingTable"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="3dp"
                            android:layout_weight="1"
                            android:background="@drawable/table_back"
                            android:padding="5dp" >

                            <TableRow
                                android:id="@+id/TableRow23"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="0"
                                android:padding="5dp"
                                android:shrinkColumns="*"
                                android:stretchColumns="*" >

                                <TextView
                                    android:id="@+id/TextView34"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center_horizontal"
                                    android:text="@string/missing_title_table"
                                    android:textAppearance="@style/tableTitleText" />
                            </TableRow>

                            <TableRow
                                android:id="@+id/TableRow18"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:layout_weight="0"
                                android:padding="5dp"
                                android:shrinkColumns="*"
                                android:stretchColumns="*" >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center_horizontal" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="@string/missing_title_col1"
                                    android:textAppearance="@style/tableTitleColumnText" />

                                <TextView
                                    android:id="@+id/TextView49"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="@string/missing_title_col2"
                                    android:textAppearance="@style/tableTitleColumnText" />
                            </TableRow>
                        </TableLayout>
                    </LinearLayout>

                </LinearLayout>

            </ScrollView>

           

        </LinearLayout>
